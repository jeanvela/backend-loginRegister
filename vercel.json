{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "./src/routes/poems.routes.js"
        },
        {
            "src": "/api/allPoems",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/poems",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/poems/:id",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/poems/post",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/poems/:id",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/poems/:id",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/register",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/login",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/logout",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/profile",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/verify",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}